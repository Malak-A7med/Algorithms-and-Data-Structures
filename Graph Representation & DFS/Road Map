///// _Malak_A7med_ /////
#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;

int n, r1, r2;
vector<vector<int>> adj;
vector<int> parent;

void dfs(int u, int p) {
    parent[u] = p;
    for (auto v : adj[u]) {
        if (v != p)
            dfs(v, u);
    }
}

void DebugOrDie() {
    cin >> n >> r1 >> r2;
    adj.assign(n + 1, {});
    parent.assign(n + 1, 0);

    for (int i = 1; i <= n; i++) {
        if (i == r1) continue;
        int p;
        cin >> p;
        adj[i].pb(p);
        adj[p].pb(i);
    }

    dfs(r2, 0);

    for (int i = 1; i <= n; i++) {
        if (i == r2) continue;
        cout << parent[i] << " ";
    }
    cout << el;
}

int main() {
    MalakA7med
    DebugOrDie();
    return 0;
}



///// _Malak_A7med_ /////
#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;

int n;
vector<string> g;
vector<vector<int>> vis;
int dx[8] = {1, -1, 0, 0, 1, 1, -1, -1};
int dy[8] = {0, 0, 1, -1, 1, -1, 1, -1};

void dfs(int x, int y) {
    vis[x][y] = 1;
    for (int k = 0; k < 8; k++) {
        int nx = x + dx[k];
        int ny = y + dy[k];
        if (nx >= 0 && nx < 2 && ny >= 0 && ny < n && !vis[nx][ny] && g[nx][ny] == '0') {
            dfs(nx, ny);
        }
    }
}

void DebugOrDie() {
    cin >> n;
    g.resize(2);
    cin >> g[0] >> g[1];
    vis.assign(2, vector<int>(n, 0));
    if (g[0][0] == '0') dfs(0, 0);
    cout << (vis[1][n-1] ? "YES" : "NO") << el;
}

int main() {
    MalakA7med
    int t; cin >> t;
    while (t--) {
        DebugOrDie();
    }
    return 0;
}
// https://codeforces.com/problemset/problem/1598/A

#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;
vector<vector<int>> adj;
vector<bool> vis;

void DFS(int u) {
    vis[u] = 1;
    cout << u << " ";
    for (auto v : adj[u]) {
        if (!vis[v]) {
            DFS(v);
        }
    }
}
void DebugOrDie() {
    int n, m; cin >> n >> m;
    adj.assign(n + 1, {});
    vis.assign(n + 1, 0);
    for (int i = 0; i < m; i++) {
        int u, v; cin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    DFS(1);
}
int main() {
    MalakA7med
    int t = 1; //cin >> t;
    while (t--) {
        DebugOrDie();
    }
    return 0;
}

///// _Malak_A7med_ /////
#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;

vector<vector<int>> adj;
vector<bool> vis;

void dfs(int u, int &nodes, int &deg2) {
    vis[u] = true;
    nodes++;
    if ((int)adj[u].size() == 2) deg2++;
    for (int v : adj[u]) {
        if (!vis[v]) dfs(v, nodes, deg2);
    }
}

void DebugOrDie() {
    int n, m;
    cin >> n >> m;
    adj.assign(n+1, vector<int>());
    vis.assign(n+1, false);

    for (int i = 0; i < m; i++) {
        int u, v;
        cin >> u >> v;
        adj[u].pb(v);
        adj[v].pb(u);
    }

    int ans = 0;
    for (int i = 1; i <= n; i++) {
        if (!vis[i]) {
            int nodes = 0, deg2 = 0;
            dfs(i, nodes, deg2);
            if (nodes == deg2 && nodes >= 3) ans++;
        }
    }

    cout << ans << el;
}

int main() {
    MalakA7med
    int t = 1; //cin >> t;
    while (t--) {
        DebugOrDie();
    }
    return 0;
}


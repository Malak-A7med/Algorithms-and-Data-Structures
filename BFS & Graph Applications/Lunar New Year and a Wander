#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;

int n, m;
vector<string> grid;
int dx[] = {0, 0, -1, 1, 1, 1, -1, -1};
int dy[] = {-1, 1, 0, 0, 1, -1, 1, -1};
bool valid(int x, int y) {return x>=0 && x<n && y>=0 && y<m && grid[x][y]=='.';}

void BFS() {
    cin >> n >> m;
    vector<vector<int>> adj(n + 1);
    for (int i = 0; i < m; ++i) {
        int a, b; cin >> a >> b;
        adj[a].pb(b);
        adj[b].pb(a);
    }
    vector<bool>vis(n + 1,false);
    vector<int> ans;
    priority_queue<int, vector<int>, greater<int>> pq;
    pq.push(1);
    vis[1] = true;
    while(!pq.empty()){
    int nd = pq.top();
    pq.pop();
    ans.pb(nd);
    for (int nxt : adj[nd]) {
        if (!vis[nxt]) {
            vis[nxt] = true;
            pq.push(nxt);
        }
    }
}
    for (int i = 0; i < ans.size(); i++) {
        cout << ans[i] << " ";
    }
}
int main() {
    MalakA7med
    int tt = 1; //cin >> tt;
    while (tt--) BFS();
    return 0;
}


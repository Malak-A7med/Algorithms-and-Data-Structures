#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;

int n, m;
vector<int>levels, vis, traversal_order;
vector<vector<int>>adj;

void BFS(int st) {
    queue<int>q;
    q.push(st);
    vis[st] = 1;
    levels[st] = 0;

    while (!q.empty()) {
        int cur = q.front();
        q.pop();

        traversal_order.pb(cur);
        for (auto ch : adj[cur]) {
            if (!vis[ch]) {
                q.push(ch);
                vis[ch] = 1;
                levels[ch] = levels[cur] + 1;
            }
        }
    }
}
void GO() {
    levels.resize(n+1);
    adj.resize(n+1);
    vis.resize(n+1);

    int u, v;
    for(int i=0; i<m; i++) {
        cin >> u >> v;
        adj[u].pb(v);
        adj[v].pb(u);
    }

    BFS(1);

    for (auto &v : traversal_order) cout << v << " ";
}

int main() {
    MalakA7med
    int tt = 1; //cin >> tt;
    while (tt--) {
        GO();
    }
    return 0;
}

///// _Malak_A7med_ /////
#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define el "\n"
using namespace std;
void solve() {
    int n, k; cin>>n>>k;
    vector<int> v(n);
    for (auto &i :v)cin>>i;
    ll ans = 0;
    for (int b=30; b>=0; b--){
        int no = 0;
        for (int j=0; j < n; j++) {
            if ((v[j] & (1 << b)) == 0) {
                no++;
            }
        }
        if (no <= k) {
            k -= no;
            ans += (1 << b);
        }
    }
    cout<<ans<<el;

}

int main() {
    MalakA7med
    int t = 1; cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}


#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define el "\n"
using namespace std;
ll factorial(ll n) {
    // base case
    if (n == 0 || n == 1) {
        return 1;
    }

    // state >> n
    return n * factorial(n-1);
}
void solve() {
    ll n; cin>>n;
    cout << factorial(n);
}
int main() {
    MalakA7med
    int t = 1; //cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}



#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define el "\n"
using namespace std;

int n, m;
string grid[101];
string res;

void Backtrack(int x, int y) {
    if (x == n-1 && y == m-1) {
        cout << res << el;
        return;
    }

    if (y + 1 < m && grid[x][y+1] != '#') {
        res.push_back('R');
        Backtrack(x, y+1);
        res.pop_back(); 
        }

    if (x + 1 < n && grid[x+1][y] != '#') {
        res.push_back('D');
        Backtrack(x+1, y);
        res.pop_back();
    }
}

void solve() {
    cin >> n >> m;
    for (int i = 0; i < n; i++) cin >> grid[i];
    Backtrack(0, 0);
}
/*
    3 3
    S..
    .#.
    ..E
ans =
    RRDD
    DDRR
 */

int main() {
    MalakA7med
    int t = 1; //cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}

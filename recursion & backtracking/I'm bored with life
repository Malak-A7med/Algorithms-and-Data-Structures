#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define pb push_back
#define el "\n"
using namespace std;
ll fact(ll n) {
    if (n <= 1) return 1;
    return n * fact(n - 1);
}

void DebugOrDie() {
    ll A, B;
    cin >> A >> B;
    ll x = min(A, B);
    cout << fact(x) << el;
}

int main() {
    MalakA7med
    int t = 1; // cin >> t;
    while (t--) {
        DebugOrDie();
    }
    return 0;
}

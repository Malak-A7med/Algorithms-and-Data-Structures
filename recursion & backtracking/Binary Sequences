#include <bits/stdc++.h>
#define all(v) v.begin(),v.end()
#define allr(v) v.rbegin(), v.rend()
#define MalakA7med ios_base::sync_with_stdio(false);cin.tie(nullptr); cout.tie(nullptr);
#define ll long long
#define el "\n"
using namespace std;
int n;
void BT(string s) {
    if (s.size() == n) {
        cout << s << el;
        return;
    }
    BT(s + "0");
    BT(s + "1");

}


void solve() {
    cin>>n;
    BT("");
  }
int main() {
    MalakA7med
    int t = 1; cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}



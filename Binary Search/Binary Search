///// _Malak_A7med_ /////
#include <iostream>
#include <algorithm>
#include <vector>
#define ll long long
#define Malak ios_base::sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);
#define all(v) v.begin(),v.end()
#define el "\n"
using namespace std;
void solve() {
    int n, q; cin >> n >> q;
    vector<int>v(n);
    for (auto& i : v) cin >> i;
    while (q--) {
        ll x; cin >> x;
        int l = 0, r = n - 1, ans = -1;
        while (l <= r) {
            int mid = (l + r) / 2;
            if (v[mid] == x) {
                ans = mid + 1; // found at 1-based index
                break;
            }
            else if (v[mid] < x) {
                l = mid + 1;
            }
            else {
                r = mid - 1;
            }
        }
        cout << ans << el;
    }
}

int main() {
    Malak
    int t = 1;  //cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}

